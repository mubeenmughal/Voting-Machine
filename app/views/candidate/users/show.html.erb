<div class="container p-5">
  <h1>Voters</h1>
  <ul class="list-group">

      <%if @constituencies.nil? %>
        <li class="list-group-item">no voters</li>
      <%else%>
        <% @constituencies.each do |const|%>
          <li class="list-group-item bg-success ">Constituency: <%= const.name%></li>
          <% if const.users.nil? %>
            <li class="list-group-item">no voters</li>
            <% else%>
              <% const.users.each do |user|%>
                <li class="list-group-item">name: <%= user.name%></li>
              <% end %>
          <% end %>
          <% end %>
      <%end%>

  </ul>
</div>
